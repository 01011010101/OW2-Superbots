rule("MERCY Charm Debuff")
{
	event
	{
		Ongoing - Each Player;
		Team 2;
		All;
	}

	conditions
	{
		Event Player.MercyCharm == 1;
	}

	actions
	{
		Set Status(Event Player, Players On Hero(Hero(Mercy), Team 1), Stunned, 5);
		Wait(5, Abort When False);
		Event Player.MercyCharm = 0;
	}
}


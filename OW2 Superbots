rule("Doomfist HUD [Updated Arkhanglesk#11200]")
{
	event
	{
		Ongoing - Each Player;
		Team 1;
		Doomfist;
	}

	actions
	{
		Wait(0.500, Ignore Condition);
		Disallow Button(Event Player, Button(Ultimate));
		Event Player.HUDTextHidden = Array(False, True, True, True);
		"Set Hero HUD text."

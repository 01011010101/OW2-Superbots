			Event Player, Knocked Down) || Has Status(Event Player, Stunned) || Has Status(Event Player, Asleep)) == True;
	}

	actions
	{
		Wait(1, Ignore Condition);
		Start Rule(ClearAllStatus, Restart Rule);
	}
}

